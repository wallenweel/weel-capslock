@echo offsetlocal enabledelayedexpansionset ahk2exe="C:\Program Files\AutoHotkey\Compiler\Ahk2Exe.exe"set app_path=.set app_script="%app_path%\Weel CapsLock.ahk"set app_exe="Weel CapsLock.exe"set app_icon="%app_path%\Weel CapsLock.ico"REM :: Kill app if is runningTaskList|Findstr /i %app_exe% > NulIf %ErrorLevel% == 0 (  taskkill /f /t /im %app_exe%)%ahk2exe% /in %app_script% /icon %app_icon%